	// Section Ready
	section.section-ready.road.road--center
		.container
			.section-ready__title-box
				h1.h1.section-ready__title Готовые
					|
					|
					strong.text-blue.strong.text-blue складские решения
				a.button-arrow.section-ready__arrow-button.section-ready__arrow-button--desktop(href="#") Все индустрии
			.simple-card-row
				// Card
				a.simple-card.section-ready__card.simple-card-row__card(href="#")
					h3.simple-card__title Fashion
					img.simple-card__bg(src="images/simple-card/img1.jpg", alt)
				// Card
				a.simple-card.section-ready__card.simple-card-row__card(href="#")
					h3.simple-card__title Healthcare
					img.simple-card__bg(src="images/simple-card/img2.jpg", alt)
				// Card
				a.simple-card.section-ready__card.simple-card-row__card(href="#")
					h3.simple-card__title Automotive
					img.simple-card__bg(src="images/simple-card/img3.jpg", alt)
				.simple-card-row__links
					a.simple-card-row__link(href="#") Telecom
					a.simple-card-row__link(href="#") POSM
					a.simple-card-row__link(href="#") FMCG
			a.button-arrow.section-ready__arrow-button.section-ready__arrow-button--mobile(href="#") Все индустрии
	// Section Services
	section.section-services
		.container
			.section-services__title-box
				h2.h2.section-services__title Продукты и услуги
					|
					|
					span.text-blue Ma
						span.text-red j
						| or
					| Terminal
				a.button-arrow.section-services__title-btn.section-services__title-btn--desktop(href="#") Все услуги
			.free-slider.swiper.overflow-visible
				.section-services__row.free-slider__wrapper.swiper-wrapper
					// Link
					a.section-services__link.swiper-slide(href="#")
						.section-services__icon-box
							img.section-services__icon(src="images/section-services/icon1.svg", alt)
						h3.section-services__link-title
							|Ответственное
							br
							|Хранение
					// Link
					a.section-services__link.swiper-slide(href="#")
						.section-services__icon-box
							img.section-services__icon(src="images/section-services/icon2.svg", alt)
						h3.section-services__link-title
							|Складская
							br
							|обработка
					// Link
					a.section-services__link.swiper-slide(href="#")
						.section-services__icon-box
							img.section-services__icon(src="images/section-services/icon3.svg", alt)
