	// Section Ready
	section.section-ready.road
		.container
			.section-ready__title-box
				h1.h1.section-ready__title Готовые
					|
					|
					strong.text-blue.strong.text-blue складские решения
				a.button-arrow(href="#") Все индустрии
			.simple-card-row
				// Card
				a.simple-card(href="#")
					h3.simple-card__title Fashion
					img.simple-card__bg(src="images/simple-card/img1.jpg", alt)
				// Card
				a.simple-card(href="#")
					h3.simple-card__title Healthcare
					img.simple-card__bg(src="images/simple-card/img2.jpg", alt)
				// Card
				a.simple-card(href="#")
					h3.simple-card__title Automotive
					img.simple-card__bg(src="images/simple-card/img3.jpg", alt)
				.simple-card-row__links
					a.simple-card-row__link(href="#") Telecom
					a.simple-card-row__link(href="#") POSM
					a.simple-card-row__link(href="#") FMCG

	// Section Services
	section.section-services
		.container
			.section-services__title-box
				h2.h2.section-services__title Услуги
					|
