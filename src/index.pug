							// News
							.news-row__card-box
								a.news-card(href="#")
									img.news-card__img(src="images/common/news6.jpg", alt)
									.news-card__content-box
										time.news-card__time(datetime="2022-09-06") 06 октября 2022 г.
										p.news-card__title Съёмки для корпоративного блога Inventive Retail Group в Логопарке Major Terminal
							// News
							.news-row__card-box
